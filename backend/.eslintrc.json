{
  "root": true,
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  /*
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  */
  "extends": ["prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "linebreak-style": 0,
    "prettier/prettier": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "no-await-in-loop": 0,
    "import/extensions": 0,
    "@typescript-eslint/return-await": 0,
    "arrow-body-style": 0,
    "import/no-unresolved": 0
  }
}
