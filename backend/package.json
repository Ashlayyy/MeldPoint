{
  "name": "meldpoint-backend",
  "version": "1.1.0",
  "description": "The API For Intalligance",
  "main": "dist/index.js",
  "scripts": {
    "test": "concurrently \"cross-env NODE_ENV=testing jest --detectOpenHandles\" \"npm:dev\"",
    "clean": "rimraf ./dist && rimraf ./coverage && npm run clean-install",
    "code:check": "concurrently \"npm:prettier:check\" \"npm:lint\" \"npm:audit\"",
    "code:clean": "concurrently \"npm:prettier:write\"",
    "copy-templates": "tsx scripts/copy-templates.ts",
    "build": "npm run clean && npm run audit && npm run code:clean && ts-patch install -s && cross-env NODE_ENV=production tsc --project tsconfig.json && npm run copy-templates",
    "start": "cross-env NODE_ENV=production node ./dist/src/server.js",
    "start:pm2": "pm2 start ./dist/src/server.js --name 'aluminate-server' -i max",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "dev:prod": "cross-env NODE_ENV=production nodemon ./src/server.ts",
    "debug": "cross-env NODE_ENV=development DEBUG=* nodemon ./src/server.ts",
    "generateDev": "prisma generate --no-hints && npm run dev",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier:check": "prettier ./src --check",
    "prettier:write": "prettier ./src --write",
    "audit": "better-npm-audit audit",
    "clean-install": "rimraf ./package-lock.json && rimraf ./node_modules && npm install && prisma generate --no-hints",
    "build-start": "npm run build && npm run start",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage"
  },
  "prisma": {
    "schema": "./prisma/schema/"
  },
  "author": "Ashlay Steur",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "@types/handlebars": "^4.0.40",
    "@types/lodash": "^4.17.14",
    "@types/multer": "^1.4.12",
    "@types/qs": "^6.9.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "apicache": "^1.6.3",
    "argon2": "^0.41.1",
    "axios": "^1.7.2",
    "better-npm-audit": "^3.7.3",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "date-fns": "^4.1.0",
    "debug": "^4.3.7",
    "deep-diff": "^1.0.2",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^9.0.0",
    "exceljs": "^4.4.0",
    "express": "^5.0.0",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.1",
    "express-slow-down": "^2.0.3",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "mongoose": "^8.14.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-microsoft": "^2.1.0",
    "qs": "^6.14.0",
    "rotating-file-stream": "^3.2.5",
    "sequelize": "^6.37.3",
    "socket.io": "^4.8.1",
    "tedious": "^18.2.1",
    "ts-node": "^10.9.2",
    "uploadthing": "^7.3.0",
    "uuid": "^11.0.5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-mongodb": "^6.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@prisma/client": "^6.8.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/apicache": "^1.6.6",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.17",
    "@types/deep-diff": "^1.0.5",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/express-slow-down": "^1.3.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.8",
    "@types/node": "^22.7.6",
    "@types/nodemailer": "^6.4.16",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/passport-microsoft": "^1.0.3",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "mongodb-memory-server": "^10.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.3",
    "prisma": "^6.8.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-patch": "^3.3.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*.ts",
      "!src/**/__tests__/**/*.ts"
    ]
  }
}
